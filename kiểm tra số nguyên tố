#include <iostream>
using namespace std;
int main()
{
    int snt;
    cout << "Nhap mot so: ";
    cin >> snt;

    int i;
    i = 1;

    while (i < snt) {
        i++;
        if (snt % i == 0) { break; }
    }

    if (snt == i) {
        cout << "Day la so nguyen to";
    }
    else { cout << "Day khong phai la so nguyen to"; }
}
