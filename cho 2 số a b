#include <iostream>
#include <string>
using namespace std;

int main()
{
   int a(0);
   int b(0);
   
   cout << "Nhập số a: ";
   cin >> a;
   cout << "Nhập số b: ";
   cin >> b;
   
   cout << "Tổng là: " <<a+b<< "\n";
   cout << "Hiệu là: " <<a-b<< "\n";
   cout << "Tích là: " <<a*b<< "\n";
   cout << "Thương là: " <<a/b<< "\n";
}
