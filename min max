#include <iostream>
using namespace std;
int main()
{
    int a, b, c;
    cout << "Nhap a, b, c: ";
    cin >> a;
    cin >> b;
    cin >> c;

    int max = a;
    if (b > max) {max = b;}
    if (c > max) { max = c; }
    cout << "So lon nhat la: " << max <<  "\n";
      
    int min = a;
    if (b < min) { min = b;}
    if (c < min) { min = c;}
    cout << "So be nhat la: " << min << "\n";
}
